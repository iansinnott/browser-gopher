  "version": "0.2.0",
  "configurations": [
    {
      "name": "search",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "search",
        "githu*"
      ]
    },
    {
      "name": "dev bleve-search",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "dev",
        "bleve-search"
      ]
    },
    {
      "name": "dev reindex",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "dev",
        "reindex"
      ]
    },
    {
      "name": "populate",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "populate",
        "--browser=vivaldi",
        "--fulltext"
      ]
    },
    {
      "name": "dev full-text",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "dev",
        "full-text",
        "https://old.reddit.com/r/vscode/comments/qromfk/comment/hka2z0n/?utm_source=reddit&utm_medium=web2x&context=3"
      ]
    }
  ]
}